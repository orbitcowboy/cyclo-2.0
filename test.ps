%!PS
%%Creator: cyclo - Control flow graph
/node {
dup
xconst mul
/x exch def			
numnodes 1 add exch sub
yconst mul
/y exch def

% centre string
dup stringwidth pop
2 div       
x exch sub /xx exch def
fontsize
3 div                    % why 3 I wonder?
y exch sub /yy exch def 
xx yy moveto
show

%draw circle
newpath  % remove line to start of circle
currentlinewidth
bubble setlinewidth
x y radius 0 360 arc stroke
setlinewidth
} def

/factor 2 sqrt def

/top {  % nodeno -top- xcoord ycoord
dup
xconst mul /x exch def
numnodes 1 add exch sub
yconst mul /y exch def
x radius factor div add y radius factor div add
} def

/bottom {  % nodeno -bottom- xcoord ycoord
dup
xconst mul /x exch def
numnodes 1 add exch sub
yconst mul /y exch def
x radius factor div sub y radius factor div sub
} def 

/toploop { % nodeno -toploop- xcoord ycoord
dup
xconst mul /x exch def
numnodes 1 add exch sub
yconst mul /y exch def
x y radius add
} def

/rightloop { % nodeno -rightloop- xcoord ycoord
dup
xconst mul /x exch def
numnodes 1 add exch sub
yconst mul /y exch def
x radius add y
} def

/arcback {
/second exch def
/first exch def
currentlinewidth
newpath
linewid setlinewidth
first bottom moveto

first bottom pop /xxx exch def
second bottom exch pop /yyy exch def

xxx 
yyy dup first bottom exch pop sub abs fudge mul add
xxx dup second bottom pop sub abs fudge mul add
yyy

second bottom
curveto
stroke

% arrow head
first bottom moveto
first bottom pop 0.25 radius mul arrow mul add
first bottom exch pop radius arrow mul sub
lineto

first bottom moveto
first bottom pop 0.25 radius mul arrow mul sub
first bottom exch pop radius arrow mul sub
lineto

stroke

setlinewidth
} def

/arcloop {
/n exch def
currentlinewidth
newpath
linewid setlinewidth
n toploop /ytop exch def /xtop exch def
n rightloop /yright exch def /xright exch def

xright yright moveto
xright radius loopfactor mul add yright radius loopfactor mul 6 div add
xtop ytop radius loopfactor mul add
xtop ytop
curveto
stroke

% arrow head
xtop ytop moveto
xtop 0.25 radius mul arrow mul add
ytop radius arrow mul add
lineto

xtop ytop moveto
xtop 0.25 radius mul arrow mul sub
ytop radius arrow mul add
lineto

stroke
setlinewidth
} def 

/arcfore {
/second exch def
/first exch def
currentlinewidth
newpath
linewid setlinewidth
first top moveto

second top pop /xxx exch def
first top exch pop /yyy exch def

xxx dup first bottom pop sub abs fudge mul sub
yyy
xxx 
yyy dup second bottom exch pop sub abs fudge mul sub


second top
curveto
stroke

% arrow head
second top moveto
second top pop radius arrow 0.25 mul mul sub
second top exch pop radius arrow mul add
lineto

second top moveto
second top pop radius arrow 0.25 mul mul add
second top exch pop radius arrow mul add
lineto

stroke

setlinewidth
} def

/xconst    10 def       % distance bubbles are apart (x)
/yconst    10 def       % distance bubbles are apart (y)
/radius     6 def       % radius of bubble
/loopfactor 2 def       % multiply by radius for loopback distance
/fontsize   6 def       % point size of text in bubbles
/fudge     .4 def       % flatness of lines between bubbles
/linewid  0.2 def       % width of lines between bubbles
/bubble   0.6 def       % width of circle around bubbles 
/arrow    0.6 def       % arrowhead length as proportion of bubble radius

